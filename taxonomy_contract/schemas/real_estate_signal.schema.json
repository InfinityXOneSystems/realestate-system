{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RealEstateSignal",
  "type": "object",
  "required": ["type", "address", "signal", "confidence"],
  "properties": {
    "type": { "type": "string" },
    "address": { "type": "string" },
    "signal": { "type": "string" },
    "confidence": { "type": "number", "minimum": 0, "maximum": 1 },
    "source": { "type": "string" },
    "timestamp": { "type": "string", "format": "date-time" }
  }
}
